head 
  title HyperWeather JSON Data
  meta(charset="utf-8")
  meta(http-equiv="X-UA-Compatible" content="IE=edge")
  meta(name="viewport" content="width=device-width, initial-scale=1")
  
  link(rel="stylesheet" href="/style.css")
  
  script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0/jquery.min.js")
  script(src="renderjson.js")
  
body

  #renderjson
  
  script.
    $(function(){
      json = JSON.parse($('#json').text());
      // console.log(json);
      
      rendered = renderjson.set_icons('\u25BE', '\u25B8')
                           .set_show_to_level(3)
                           (json);
      
      document.getElementById("renderjson").appendChild(rendered);
    });
    
  div#json.hidden!= JSON.stringify(json)
    
